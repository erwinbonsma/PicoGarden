 state.num_revives=0
 state.num_decays=0
 state.num_mutations=0
 state.btnx_hold=0
 state.fadein=32
 state.target_wait=state.wait
 state.wait=5
 state.history:reset()

 _draw=main_draw
 _update=main_update
end

