 local a=d0+64*32
 for y=32,95 do
  memcpy(a,a+40,12)
  memcpy(a+52,a+12,12)
  a+=64
 end

 if state.flash_bg>0 then
  for y=32,95 do
   local a=d0+y*64
   local b=a+52
   for x=1,12 do
    poke(a,flash[@a])
    poke(b,flash[@b])
    a+=1
    b+=1
   end
  end
 end

 local a=d0
 local b=d0+64*96
